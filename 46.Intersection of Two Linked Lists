
public class Solution {
    public ListNode getIntersectionNode(ListNode headA, ListNode headB) {
        ListNode temp1 = headA;
         ListNode temp2 = headB;
         int size1 =0;
         int size2 =0;
         while(temp1 != null){
            size1++;
            temp1= temp1.next;
         }
         while(temp2 != null){
            size2++;
            temp2= temp2.next;
         }

        ListNode node1 = headA;
         ListNode node2 = headB;
         int step =0;
         if(size1 >= size2){
            step= size1-size2;
            for(int i =0 ; i<step ; i++){
                node1=node1.next;
            }
             while(node1 != null && node2 != null){

                   if(node1==node2){
                     return node1;
                     }
            node1=node1.next;
            node2=node2.next;   
        }
         }
        else {
            step= size2-size1;
             for(int i =0 ; i<step ; i++){
                node2=node2.next;
            } 
             while(node1 != null && node2 != null ){

                   if(node1==node2){
                     return node2;
                     }
            node2=node2.next;
            node1=node1.next;

        }
         }


    return null;
    }
}
